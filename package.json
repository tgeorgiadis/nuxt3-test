  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "lint": "eslint --ext .ts,.js,.vue .",
    "ionic:build": "yarn generate",
    "ionic:serve": "yarn dev",
    "ionic:run": "ionic capacitor run android --livereload --external",
