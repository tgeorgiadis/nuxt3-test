    "eslint": "^8.15.0",
    "eslint-plugin-vue": "^8.7.1",
    "nuxt": "3.0.0-rc.2",
    "prisma": "^3.13.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@capacitor/android": "^3.5.0",
    "@capacitor/core": "^3.5.0",
    "@prisma/client": "^3.13.0"
  }
